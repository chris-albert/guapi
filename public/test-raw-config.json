{
  "display": "Test Root",
  "settings": [
    {
      "name": "env",
      "display": "Environment"
    }
  ],
  "auth": {
    "request": {
      "url": "https://{{settings.env}}.ticketfly.com",
      "path": "/account/oauth2/token",
      "method": "POST",
      "location": "form",
      "root": "",
      "auth": {
        "type": "basic",
        "user": "clientId",
        "pass": "clientSecret"
      },
      "submit": {
        "display": "Login",
        "type": "default",
        "size": "xs"
      },
      "fields": [
        {
          "name": "username",
          "display": "Username",
          "type": "text"
        },
        {
          "name": "password",
          "display": "Password",
          "type": "password"
        },
        {
          "name": "clientId",
          "display": "Client Id",
          "type": "text"
        },
        {
          "name": "clientSecret",
          "display": "Client Secret",
          "type": "text"
        },
        {
          "name": "grant_type",
          "type": "hidden",
          "value": "password"
        }
      ],
      "settingFields": [
        "clientId",
        "clientSecret"
      ]
    },
    "response": {
      "root": "",
      "type": "none",
      "fields": []
    },
    "dataFilter": [
      "clientId",
      "clientSecret"
    ]
  },
  "content": {
    "type": "tabs",
    "tabs": [
      {
        "display": "Tab 1",
        "name": "tab1",
        "content": {
          "type": "tabs",
          "tabs": [
            {
              "display": "Nest Tab 1",
              "name": "nestTab1",
              "content": {
                "type": "form",
                "request": {
                  "url": "https://{{settings.env}}.google.com/{{settings.env}}",
                  "path": "/test/path",
                  "method": "POST",
                  "location": "query",
                  "auth": {
                    "type": "bearer"
                  },
                  "submit": {
                    "display": "Submit Text",
                    "type": "primary",
                    "size": "sm"
                  },
                  "fields": [
                    {
                      "display": "Test Text",
                      "name": "testText",
                      "type": "text"
                    },
                    {
                      "display": "Test Password",
                      "name": "testPassword",
                      "type": "password"
                    },
                    {
                      "display": "Test Disabled",
                      "name": "testDisabled",
                      "type": "text",
                      "disabled": true
                    },
                    {
                      "display": "Test Select",
                      "name": "testSelect",
                      "type": "select",
                      "value": "option1",
                      "values": [
                        {
                          "display": "Option 1",
                          "name": "option1"
                        },
                        {
                          "display": "Option 2",
                          "name": "option2"
                        }
                      ]
                    },
                    {
                      "display": "Test Multi Select",
                      "name": "testMultiSelect",
                      "type": "select",
                      "multi": true,
                      "value": ["option1"],
                      "values": [
                        {
                          "display": "Option 1",
                          "name": "option1"
                        },
                        {
                          "display": "Option 2",
                          "name": "option2"
                        }
                      ]
                    }
                  ]
                },
                "response": {
                  "root": "employees",
                  "type": "array",
                  "fields": "*"
                }
              }
            },
            {
              "display": "Nest Tab 2",
              "name": "nestTab2",
              "content": {
                "type": "form",
                "request": {
                  "url": "http://google.com/{{settings.env}}",
                  "path": "/test/path",
                  "method": "POST",
                  "location": "query",
                  "auth": {
                    "type": "bearer"
                  },
                  "submit": {
                    "display": "Submit Text",
                    "type": "primary",
                    "size": "sm"
                  },
                  "fields": [
                    {
                      "display": "Test Text",
                      "name": "testText",
                      "type": "text"
                    },
                    {
                      "display": "Test Password",
                      "name": "testPassword",
                      "type": "password"
                    }
                  ]
                },
                "response": {
                  "root": "employees",
                  "type": "array",
                  "fields": "*"
                }
              }
            }
          ]
        }
      },
      {
        "display": "Tab 2",
        "name": "tab2",
        "content": {
          "type": "tabs",
          "tabs": [
            {
              "display": "Nest Tab 1",
              "name": "nestTab1",
              "content": {
                "type": "tabs",
                "tabs": [
                  {
                    "display": "2 Nests Tab 1",
                    "name": "2NestsTab1",
                    "content": {
                      "type": "form",
                      "request": {
                        "url": "http://google.com/{{settings.env}}",
                        "path": "/test/path",
                        "method": "POST",
                        "location": "query",
                        "auth": {
                          "type": "bearer"
                        },
                        "submit": {
                          "display": "Submit Text",
                          "type": "primary",
                          "size": "sm"
                        },
                        "fields": [
                          {
                            "display": "Test Text",
                            "name": "testText",
                            "type": "text"
                          },
                          {
                            "display": "Test Password",
                            "name": "testPassword",
                            "type": "password"
                          }
                        ]
                      },
                      "response": {
                        "root": "employees",
                        "type": "array",
                        "fields": "*"
                      }
                    }
                  }
                ]
              }
            }
          ]
        }
      }
    ]
  }
}
