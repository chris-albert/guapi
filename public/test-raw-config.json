                  "url": "https://{{settings.env}}.ticketfly.com",
                  "path": "/account/oauth2/token",
                  "method": "POST",
                  "location": "form",
                  "auth": {
                    "type": "basic",
                    "user": "settings.clientId",
                    "pass": "settings.clientSecret"
                  },
                  "submit": {
                    "display": "Submit",
                    "type": "primary",
                    "size": "sm"
                  },
                  "fields": [
                    {
                      "name": "username",
                      "display": "Username",
                      "type": "text"
                    },
                    {
                      "name": "password",
                      "display": "Password",
                      "type": "password"
                    },
                    {
                      "name": "grant_type",
                      "type": "hidden",
                      "value": "password"
                    }
                  ]
                },
                "response": {
                  "root": "",
                  "type": "object",
                  "fields": "*"
                }
              }
