        {
          "name": "clientId",
          "display": "Client Id",
          "type": "text"
        },
        {
          "name": "clientSecret",
          "display": "Client Secret",
          "type": "text"
        },
        {
          "name": "grant_type",
          "type": "hidden",
          "value": "password"
        }
      ]
    },
    "response": {
      "settings": {
        "access_token": "token"
      },
      "root": "",
      "type": "none",
      "fields": []
