{
  "display": "Test Root",
  "settings": [
    {
      "name": "env",
      "display": "Environment",
      "editable": true
    },
    {
      "name": "clientId",
      "editable": false
    },
    {
      "name": "clientSecret",
      "editable": false
    }
  ],
  "auth": {
    "request": {
      "url": "https://{{settings.env}}.ticketfly.com",
      "path": "/account/oauth2/token",
      "method": "POST",
      "location": "form",
      "root": "",
      "auth": {
        "type": "basic",
        "user": "clientId",
        "pass": "clientSecret"
      },
      "submit": {
        "display": "Login",
        "type": "default",
        "size": "xs"
      },
      "fields": [
        {
          "name": "username",
          "display": "Username",
          "type": "text"
        },
        {
          "name": "password",
          "display": "Password",
          "type": "password"
        },
        {
          "name": "clientId",
          "display": "Client Id",
          "type": "text"
        },
        {
          "name": "clientSecret",
          "display": "Client Secret",
          "type": "text"
        },
        {
          "name": "grant_type",
          "type": "hidden",
          "value": "password"
        }
      ]
    },
    "response": {
      "settings": {
        "access_token": "token"
      },
      "root": "",
      "type": "none",
      "fields": []
    },
    "dataFilter": [
      "clientId",
      "clientSecret"
    ]
  },
  "content": {
    "type": "tabs",
    "tabs": [
      {
        "display": "Permissions",
        "name": "permissions",
        "content": {
          "type": "tabs",
          "tabs": [
            {
              "display": "Employees",
              "name": "employees",
              "content": {
                "type": "tabs",
                "tabs": [
                  {
                    "display": "List",
                    "name": "list",
                    "content": {
                      "type": "form",
                      "request": {
                        "url": "https://authorization-{{settings.env}}.ticketfly.com",
                        "path": "/aggregator/v1/employees",
                        "method": "GET",
                        "location": "query",
                        "auth": {
                          "type": "bearer"
                        },
                        "submit": {
                          "display": "Get",
                          "type": "primary",
                          "size": "sm"
                        },
                        "fields": [
                          {
                            "display": "Domain Org Id",
                            "name": "domainOrgId",
                            "type": "text"
                          },
                          {
                            "display": "Client Org Id",
                            "name": "clientOrgId",
                            "type": "text"
                          }
                        ]
                      },
                      "response": {
                        "root": "employees",
                        "type": "array",
                        "fields": "*"
                      }
                    }
                  },
                  {
                    "display": "View",
                    "name": "view",
                    "content": {
                      "type": "form",
                      "request": {
                        "url": "https://authorization-{{settings.env}}.ticketfly.com",
                        "path": "/aggregator/v1/employees/{{id}}",
                        "method": "GET",
                        "location": "query",
                        "auth": {
                          "type": "bearer"
                        },
                        "submit": {
                          "display": "Get",
                          "type": "primary",
                          "size": "sm"
                        },
                        "fields": [
                          {
                            "display": "Id",
                            "name": "id",
                            "type": "text"
                          },
                          {
                            "display": "Domain Org Id",
                            "name": "domainOrgId",
                            "type": "text"
                          },
                          {
                            "display": "Client Org Id",
                            "name": "clientOrgId",
                            "type": "text"
                          }
                        ]
                      },
                      "response": {
                        "root": "employee",
                        "type": "object",
                        "fields": "*"
                      }
                    }
                  },
                  {
                    "display": "Create",
                    "name": "create",
                    "content": {
                      "type": "form",
                      "request": {
                        "url": "https://authorization-{{settings.env}}.ticketfly.com",
                        "path": "/aggregator/v1/employees",
                        "method": "POST",
                        "location": "json",
                        "root": {
                          "employee": ["id", "firstName", "lastName", "email", "role", "allEvents", "events"]
                        },
                        "auth": {
                          "type": "bearer"
                        },
                        "submit": {
                          "display": "Create",
                          "type": "primary",
                          "size": "sm"
                        },
                        "fields": [
                          {
                            "display": "Domain Org Id",
                            "name": "domainOrgId",
                            "type": "text"
                          },
                          {
                            "display": "Client Org Id",
                            "name": "clientOrgId",
                            "type": "text"
                          },
                          {
                            "display": "Id",
                            "name": "id",
                            "type": "text"
                          },
                          {
                            "display": "First Name",
                            "name": "firstName",
                            "type": "text"
                          },
                          {
                            "display": "Last Name",
                            "name": "lastName",
                            "type": "text"
                          },
                          {
                            "display": "Email",
                            "name": "email",
                            "type": "text"
                          },
                          {
                            "display": "Role",
                            "name": "role",
                            "type": "text"
                          },
                          {
                            "display": "All Events",
                            "name": "allEvents",
                            "type": "text"
                          },
                          {
                            "display": "Events",
                            "name": "events",
                            "type": "text"
                          }
                        ]
                      },
                      "response": {
                        "root": "employee",
                        "type": "object",
                        "fields": "*"
                      }
                    }
                  }
                ]
              }
            },
            {
              "display": "Roles",
              "name": "roles",
              "content": {
                "type": "tabs",
                "tabs": [
                  {
                    "display": "List",
                    "name": "list",
                    "content": {
                      "type": "form",
                      "request": {
                        "url": "https://authorization-{{settings.env}}.ticketfly.com",
                        "path": "/v2/roles",
                        "method": "GET",
                        "location": "query",
                        "auth": {
                          "type": "bearer"
                        },
                        "submit": {
                          "display": "List",
                          "type": "primary",
                          "size": "sm"
                        },
                        "fields": [
                          {
                            "display": "Id",
                            "name": "id",
                            "type": "text"
                          },
                          {
                            "display": "Name",
                            "name": "Name",
                            "type": "text"
                          },
                          {
                            "display": "Owner Org Id",
                            "name": "ownerOrgId",
                            "type": "text"
                          }
                        ]
                      },
                      "response": {
                        "root": "roles",
                        "type": "array",
                        "fields": "*",
                        "actions": [
                          {
                            "type": "icon",
                            "link": "{{route.base}}.view",
                            "params": ["id"],
                            "autoSubmit": true,
                            "icon": "info-sign"
                          }
                        ]
                      }
                    }
                  },
                  {
                    "display": "View",
                    "name": "view",
                    "content": {
                      "type": "form",
                      "request": {
                        "url": "https://authorization-{{settings.env}}.ticketfly.com",
                        "path": "/v2/roles/{{id}}",
                        "method": "GET",
                        "location": "query",
                        "auth": {
                          "type": "bearer"
                        },
                        "submit": {
                          "display": "List",
                          "type": "primary",
                          "size": "sm"
                        },
                        "fields": [
                          {
                            "display": "Id",
                            "name": "id",
                            "type": "text"
                          }
                        ]
                      },
                      "response": {
                        "root": "role",
                        "type": "object",
                        "fields": "*",
                        "actions": [
                          {
                            "type": "button",
                            "display": "Delete",
                            "link": "{{route.base}}.delete",
                            "params": ["id"]
                          }
                        ]
                      }
                    }
                  },
                  {
                    "display": "Create",
                    "name": "create",
                    "content": {
                      "type": "form",
                      "request": {
                        "url": "https://authorization-{{settings.env}}.ticketfly.com",
                        "path": "/v2/roles",
                        "method": "POST",
                        "location": "json",
                        "root": "role",
                        "auth": {
                          "type": "bearer"
                        },
                        "submit": {
                          "display": "Create",
                          "type": "primary",
                          "size": "sm"
                        },
                        "fields": [
                          {
                            "display": "Id",
                            "name": "id",
                            "type": "text"
                          },
                          {
                            "display": "Name",
                            "name": "name",
                            "type": "text"
                          },
                          {
                            "display": "Owner Org Id",
                            "name": "ownerOrgId",
                            "type": "text"
                          }
                        ]
                      },
                      "response": {
                        "root": "role",
                        "type": "object",
                        "fields": "*"
                      }
                    }
                  },
                  {
                    "display": "Delete",
                    "name": "delete",
                    "content": {
                      "type": "form",
                      "request": {
                        "url": "https://authorization-{{settings.env}}.ticketfly.com",
                        "path": "/v2/roles/{{id}}",
                        "method": "DELETE",
                        "location": "query",
                        "auth": {
                          "type": "bearer"
                        },
                        "submit": {
                          "display": "Delete",
                          "type": "primary",
                          "size": "sm"
                        },
                        "fields": [
                          {
                            "display": "Id",
                            "name": "id",
                            "type": "text"
                          }
                        ]
                      },
                      "response": {
                        "root": "role",
                        "type": "object",
                        "fields": "*"
                      }
                    }
                  }
                ]
              }
            }
          ]
        }
      },
      {
        "display": "Tab 2",
        "name": "tab2",
        "content": {
          "type": "tabs",
          "tabs": [
            {
              "display": "Nest Tab 1",
              "name": "nestTab1",
              "content": {
                "type": "tabs",
                "tabs": [
                  {
                    "display": "2 Nests Tab 1",
                    "name": "2NestsTab1",
                    "content": {
                      "type": "form",
                      "request": {
                        "url": "http://google.com/{{settings.env}}",
                        "path": "/test/path",
                        "method": "POST",
                        "location": "query",
                        "auth": {
                          "type": "bearer"
                        },
                        "submit": {
                          "display": "Submit Text",
                          "type": "primary",
                          "size": "sm"
                        },
                        "fields": [
                          {
                            "display": "Test Text",
                            "name": "testText",
                            "type": "text"
                          },
                          {
                            "display": "Test Password",
                            "name": "testPassword",
                            "type": "password"
                          }
                        ]
                      },
                      "response": {
                        "root": "employees",
                        "type": "array",
                        "fields": "*"
                      }
                    }
                  }
                ]
              }
            }
          ]
        }
      }
    ]
  }
}
