{
  "display": "TicketFly",
  "name": "ticketfly",
  "settings": [
    {
      "name": "Environment::env",
      "editable": true,
      "label": true
    },
    {
      "name": "::clientId",
      "editable": true
    }
  ],
  "auth": {
    "request": {
      "url": "https://{{settings.env}}.ticketfly.com",
      "path": "/account/oauth2/authorize",
      "method": "GET",
      "location": "query",
      "type": "link",
      "fields": [
        {
          "name": "response_type",
          "value": "token"
        },
        {
          "name": "client_id",
          "value": "{{settings.clientId}}"
        },
        {
          "name": "state",
          "value": "{{router.currentPath}}"
        },
        {
          "name": "redirect_uri",
          "value": "{{location.origin}}{{location.pathname}}#/auth?a=b"
        }
      ]
    }
  },
  "tabs": [
    "./config/projects/permissions.json",
    "./config/projects/account.json",
    "./config/projects/mod.json"
  ]
}
