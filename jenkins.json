{
  "build": [
    {
      "repoName": "eddy",
      "type": [
        "shell",
        "shell-feature"
      ],
      "nodejs": "6.4.0",
      "archiveArtifacts": "**.zip",
      "command": [
        "./jenkins.sh"
      ],
      "envPostBuild": {
        "propertiesFile": "build.properties",
        "variables": [
          "CURRENT_VERSION=${app_version}",
          "ARTIFACT_FILE=${app_name}-${app_version}.zip"
        ],
        "scriptPath": "echo generated artifact $ARTIFACT_FILE && touch $ARTIFACT_FILE"
      },
      "deployToArtifactory": "${WORKSPACE}/*.zip=>com/ticketfly/${app_name}/${app_version}/",
      "tapResultFile": "results.tap"
    },
    {
      "repoName": "eddy",
      "type": [
        "shell-pr"
      ],
      "nodejs": "6.4.0",
      "archiveArtifacts": "**.zip",
      "command": [
        "./jenkins.sh"
      ],
      "envPostBuild": {
        "propertiesFile": "build.properties",
        "variables": [
          "CURRENT_VERSION=${app_version}",
          "ARTIFACT_FILE=${app_name}-${app_version}.zip"
        ],
        "scriptPath": "echo generated artifact $ARTIFACT_FILE && touch $ARTIFACT_FILE"
      },
      "tapResultFile": "results.tap"
    }
  ]
}
