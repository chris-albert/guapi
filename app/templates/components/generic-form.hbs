{{#form-input
  fields=model.fields
  submitName=submitDisplay
  showOauth2=showOauth2Select
  authSelector=authSelector
  onSubmit=(action "submit")
  onActionClick=(action "actionClick")
  onFormChange=(action "formChange")
  formActions=model.additionalActions
}}
{{/form-input}}

{{#request-details
  request=request.current
  settings=model
}}
{{/request-details}}

{{#form-response
  response=response
  fields=model.fields
  config=model.response
  route=model.route
  request=request.lastObj
  formConfig=model
}}
{{/form-response}}


