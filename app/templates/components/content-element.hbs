{{#if isTabs}}
  {{#tab-component model=content.tabs route=route}}
    {{outlet}}
  {{/tab-component}}
{{else if isForm}}
  {{#generic-form
    model=content.form
    queryParams=queryParams
    route=route
  }}{{/generic-form}}
{{else if isText}}
  {{#text-component model=content}}{{/text-component}}
{{/if}}